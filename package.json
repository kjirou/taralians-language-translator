{
  "name": "taralians-language-translator",
  "version": "1.0.1",
  "description": "A fictional \"The Taralians Language\"(タラール語) translator",
  "main": "dist/index.js",
  "scripts": {
    "build": "$(npm bin)/babel --extensions .ts --out-dir ./dist ./src",
    "build-and-publish": "npm run build && npm publish",
    "declaration": "$(npm bin)/tsc --project ./tsconfigs/declaration.json",
    "test": "$(npm bin)/mocha --require './setup/babel-register.js' --recursive",
    "typecheck": "$(npm bin)/tsc --project ./tsconfigs/typecheck.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjirou/taralians-language-translator.git"
  },
  "keywords": [
    "game",
    "joke"
  ],
  "author": "kjirou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kjirou/taralians-language-translator/issues"
  },
  "homepage": "https://github.com/kjirou/taralians-language-translator#readme",
  "files": [
    "LICENSE",
    "README.md",
    "bin",
    "dist",
    "package.json",
    "package-lock.json"
  ],
  "bin": {
    "taralians": "bin/cli.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "mocha": "^5.0.5",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "minimist": "^1.2.0",
    "minimist-subcommand": "^2.0.0"
  }
}
